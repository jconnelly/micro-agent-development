{
  "dashboard": {
    "id": null,
    "title": "Micro-Agent APM Dashboard",
    "tags": ["micro-agent", "apm", "performance"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 100
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Response Time (P95)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 Response Time",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 2
                },
                {
                  "color": "red",
                  "value": 5
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total{status_code=~\"4..|5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "palette-classic"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Active Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "active_connections_total",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Request Duration by Endpoint",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m])) by (endpoint)",
            "legendFormat": "{{endpoint}} (P95)",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m])) by (endpoint)",
            "legendFormat": "{{endpoint}} (P50)",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Agent Operation Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(agent_processing_duration_seconds_sum[5m]) / rate(agent_processing_duration_seconds_count[5m])",
            "legendFormat": "{{agent_type}} - {{operation}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "LLM Provider Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(llm_request_duration_seconds_sum[5m]) / rate(llm_request_duration_seconds_count[5m])",
            "legendFormat": "{{provider}} - {{model}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 17
        }
      },
      {
        "id": 8,
        "title": "LLM Token Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(llm_tokens_total[5m])",
            "legendFormat": "{{provider}} - {{type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 17
        }
      },
      {
        "id": 9,
        "title": "System Resources",
        "type": "timeseries",
        "targets": [
          {
            "expr": "cpu_usage_percent",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "memory_usage_bytes{type=\"percent\"}",
            "legendFormat": "Memory Usage %",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "palette-classic"
            },
            "max": 100
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 26
        }
      },
      {
        "id": 10,
        "title": "PII Detection Activity",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(pii_detections_total[5m])",
            "legendFormat": "{{pii_type}} - {{context}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 26
        }
      },
      {
        "id": 11,
        "title": "Cache Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(cache_operations_total{result=\"hit\"}[5m]) / rate(cache_operations_total[5m]) * 100",
            "legendFormat": "Cache Hit Rate %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 35
        }
      },
      {
        "id": 12,
        "title": "Business Rules Extracted",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(business_rules_extracted_total[5m])",
            "legendFormat": "{{domain}} - {{complexity}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 35
        }
      },
      {
        "id": 13,
        "title": "Error Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (error_type) (rate(errors_total[5m]))",
            "legendFormat": "{{error_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 44
        }
      },
      {
        "id": 14,
        "title": "Request Status Codes",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (status_code) (rate(http_requests_total[5m]))",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 44
        }
      }
    ]
  }
}